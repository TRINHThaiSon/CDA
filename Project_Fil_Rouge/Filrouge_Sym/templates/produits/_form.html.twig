{{form_start(produitform)}}
         <div class="form-group">
               <label for="">Nom de produit</label>
               {{ form_widget(produitform.libelleCourteArticle)}}               
         </div>
         <div class="form-group">
            <label for="">Description</label>
            {{ form_widget(produitform.libelleLongArticle)}}
         </div>
         <div class="form-group">
            <label for="souscate">Sous Catégorie</label>
            {{ form_widget(produitform.souscategorie)}}
         </div>
        
         <div class="form-group">
            <label for="">Image</label>
            {{ form_widget(produitform.imageArticle)}}
         </div>
         <div class="form-group">
            <label for="">Prix unitaire</label>
            {{ form_widget(produitform.prixunitArticle)}}
         </div>
         <div class="form-group">
            <label for="">Quantité en stock</label>
            {{ form_widget(produitform.nombreStkArticle)}}
         </div>
         
         <div class="form-group">
            <label for="fournisseur">Fournisseur</label>
            {{ form_widget(produitform.Fournisseurs)}}
         </div>              
         
         {{ form_rest(produitform)}}
         <button type="submit" class="btn btn-primary">Submit</button>
         <button type="reset" class="btn btn-warning">Annuler</button>
       {{form_end(produitform)}} 